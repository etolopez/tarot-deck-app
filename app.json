{
  "expo": {
    "name": "Tarot",
    "slug": "tarot-deck",
    "version": "1.0.0",
    "scheme": "tarotreading",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1f3a"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "274523ae-f83c-4eff-b8a1-3d78e3c09384"
      },
      "apiBaseUrl": "http://localhost:3000",
      "solanaRecipientAddress": "66FTEkkqvKa4QMMRXaryf2oGmknTFbyLQrmKRK6qAJkC",
      "solanaCluster": "mainnet-beta",
      "solanaRpc": "",
      "enableSolanaPaymentsAndroid": true,
      "enableSolanaPaymentsIos": false
    },
    "assetBundlePatterns": [
      "**/*",
      "apps/mobile/assets/**/*"
    ],
    "ios": {
      "icon": "./assets/icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.tarotreading.app",
      "buildNumber": "1"
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1a1f3a"
      },
      "package": "com.tarotreading.app",
      "versionCode": 2,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "tarotreading"
            }
          ]
        }
      ],
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-sqlite",
      "expo-secure-store",
      "expo-audio"
    ],
    "owner": "simplyeto"
  }
}
